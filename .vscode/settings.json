{
	"c-cpp-flylint.debug": true,
	"c-cpp-flylint.language": "c",
	"c-cpp-flylint.ignoreParseErrors": true,
	"c-cpp-flylint.clang.severityLevels": {
		"fatal": "Error",
		"error": "Error",
		"warning": "Error",
		"note": "Information"
	},
	"c-cpp-flylint.clang.standard": [
		"c11"
	],
	"c-cpp-flylint.flexelint.enable": false,
	"c-cpp-flylint.flawfinder.enable": false,
	"c-cpp-flylint.lizard.enable": false,
	"c-cpp-flylint.cppcheck.enable": false,
	"C_Cpp.default.compilerPath": "/usr/bin/clang",
	"c-cpp-flylint.includePaths": [
		"${workspaceFolder}/includes",
		"${workspaceFolder}/Libft/Headers"
	],
	"C_Cpp.default.includePath": [
		"${workspaceFolder}/includes",
		"${workspaceFolder}/Libft/Headers"
	],
	"C_Cpp.default.intelliSenseMode": "linux-clang-x64",
	"editor.rulers": [
		80
	],
}